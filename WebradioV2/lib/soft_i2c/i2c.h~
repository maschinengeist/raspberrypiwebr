#ifndef __SOFT_I2C_H__
#define __SOFT_I2C_H__

void i2c_wait(unsigned long int loop);

void i2c_start(void);

void i2c_stop(void);

void i2c_restart(void);

void i2c_ack(void);

void i2c_noack(void);

unsigned char i2c_write(unsigned char value);

unsigned char i2c_read(void);

#endif
